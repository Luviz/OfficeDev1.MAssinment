@model MAssinment01App.ODA1Web.Models.Customer

<div class="row">
	<div class="col-md-3">

		<img src="@Model.Logo" height="100px"  alt="Alternate Text" />
	</div>
	<div class="col-md-9">

		<dl class="dl-horizontal ">
			<dt>Title: </dt>
			<dd> @Model.Title</dd>
			<dt>Contact Person: </dt>
			<dd> @Model.ContactPerson</dd>
			<dt>Mobile: </dt>
			<dd> @Model.Mobile</dd>
			<dt>E-mail: </dt>
			<dd> @Model.Email</dd>
			<dt>Created: </dt>
			<dd> @Model.Created</dd>
		</dl>
	</div>
</div>